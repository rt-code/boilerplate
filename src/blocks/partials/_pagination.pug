+b.pagination(class="js-pagination")
  +b.BUTTON.btn(class="--line-gray --icon-left") 
    +icon('arrow-left','')
    span Предыдущая
  +e.numbers
    +e.first
      a(href="#") 1
      a(href="#") 2
      a(href="#") 3
      a(href="#").active 4
      a(href="#") 5
    span ...
    +e.last
      a(href="#") 10
      a(href="#") 11
      a(href="#") 12
      a(href="#") 13
  +b.BUTTON.btn(class="--line-gray --icon-right") 
    span Следующая
    +icon('arrow-right','')